{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        // "plugn:react/jsx-runtime"
        "plugin:jsx-a11y/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jsx-a11y"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
		"no-console": "off",
		"semi": "error",
        "react/no-unescaped-entities": 0,
		"import/no-extraneous-dependencies": [
			"off",
			{
				"devDependencies": true
			}
		]
	}
}
